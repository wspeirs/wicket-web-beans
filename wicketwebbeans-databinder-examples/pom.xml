  <groupId>com.googlecode.wicketwebbeans.databinder.examples</groupId>
  <artifactId>wicketwebbeans-databinder-examples</artifactId>
  <name>Databinder 1.3 Examples</name>
  <description>Wicket Web Beans Databinder Examples</description>
  <packaging>war</packaging>
  <build>
