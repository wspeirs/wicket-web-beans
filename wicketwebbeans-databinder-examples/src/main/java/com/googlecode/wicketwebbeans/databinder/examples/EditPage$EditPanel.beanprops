Contact
{
	label: "Edit Contact";
	actions: save, cancel;
	props: -id,
			action.save{labelImage: "Save16.gif"},
			action.cancel{labelImage: "Undo16.gif"},
			firstName{required: true}, lastName, phoneNumber, 
			category{fieldType: com.googlecode.wicketwebbeans.databinder.examples.CategoryField};
}
Category
{
	label: "Edit Category";
	actions: save, cancel;
	props: -id, name;
}

